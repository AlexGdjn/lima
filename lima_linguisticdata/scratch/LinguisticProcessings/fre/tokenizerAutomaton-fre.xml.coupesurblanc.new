        <action>+1</action>
      </transition>
      <transition>
        <to_state>ALPHANUMERIC</to_state>
        <event>c_lowline</event>
        <action>+1</action>
      </transition>
      <transition>
        <to_state>ALPHANUMERIC</to_state>
        <event>unknwn</event>
        <action>+1</action>
        <action>trace</action>
      </transition>
      <transition>
        <to_state>START</to_state>
        <event>c_dot</event>
        <condition>
          <after>
            <event>c_b</event>
          </after>
          <after>
            <event>c_m</event>
          </after>
        </condition>
        <action>+1</action>
        <action>token</action>
      </transition>
      <transition>
        <to_state>START</to_state>
        <event>all</event>
        <action>token</action>
      </transition>
      <transition>
        <to_state>END</to_state>
        <event>m_parag</event>
        <action>token</action>
        <action>exit</action>
      </transition>
    </state>
    <state id="BLANK">
      <transition>
        <to_state>START</to_state>
        <event>c_b</event>
        <action>+1</action>
        <action>flush</action>
      </transition>
      <transition>
        <to_state>BLANK</to_state>
        <event>all</event>
        <action>+1</action>
      </transition>
    </state>
    <state id="END">
      <!-- -->
