<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="Automaton.xslt"?>
<language_definition id="FRE_DEF" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="language.xsd">
  <!-- Caution :
1) Actions can't be added without modifying C++ code.
2) Status Settings can't be added without modifying C++ code
3) Automatons which set status can't be renamed or added without modifying C++ code-->
  <automaton id="A_MAIN">
    <!-- must be A_MAIN-->
    <title>Main automaton</title>
    <!-- 4 permitted Actions : "+1", "flush", "trace", token"-->
    <!-- 8 permitted Settings : "T_ALPHA", "T_NUMERIC", "T_PATTERN", "T_WORD_BRK",
"T_SENTENCE_BRK", "T_HYPHEN_WORD", "T_POSSESSIVE", "T_ALPHANUMERIC" -->
    <state id="START">
      <transition>
        <to_state>ALPHA</to_state>
        <event>c_Mm</event>
        <condition>
          <after>
            <event>c_hyphen</event>
          </after>
          <after>
            <event>c_a_t</event>
          </after>
          <after>
            <event>c_hyphen</event>
          </after>
        </condition>
        <setting>T_ALPHA</setting>
      </transition>
      <transition>
        <to_state>ALPHA</to_state>
        <event>c_Mm</event>
        <action>+1</action>
        <setting>T_ALPHA</setting>
      </transition>
      <transition>
              <to_state>NUMERIC</to_state>
              <event>c_5</event>
              <action>+1</action>
              <setting>T_NUMERIC</setting>
      </transition>
      <transition>
              <to_state>NUMERIC</to_state>
              <event>c_hyphen</event>
              <event>c_plus</event>
              <condition>
                      <after>
                              <event>c_5</event>
                      </after>
              </condition>
              <action>+1</action>
              <setting>T_NUMERIC</setting>
      </transition>
      <transition>
        <to_state>PATTERN</to_state>
        <event>m_pattern</event>
        <action>+1</action>
        <setting>T_PATTERN</setting>
        <default>t_pattern</default>
      </transition>
      <transition>
        <to_state>START</to_state>
        <event>c_b</event>
        <action>+1</action>
        <action>flush</action>
      </transition>
      <transition>
        <to_state>START</to_state>
        <event>c_lowline</event>
        <action>+1</action>
      </transition>
      <transition>
        <to_state>START</to_state>
        <event>c_other</event>
        <action>+1</action>
        <action>trace</action>
        <action>flush</action>
      </transition>
      <transition>
        <to_state>START</to_state>
        <event>c_hyphen</event>
        <condition>
          <after>
            <event>c_a_t</event>
          </after>
          <after>
            <event>c_hyphen</event>
          </after>
        </condition>
        <action>+1</action>
        <setting>T_ALPHA</setting>
      </transition>
      <transition>
        <to_state>BLANK</to_state>
        <event>c_del2</event>
        <event>c_dot</event>
        <condition>
          <after>
            <event>c_par</event>
          </after>
          <after>
            <event>c_par</event>
          </after>
        </condition>
        <action>+1</action>
        <setting>T_PARAGRAPH_BRK</setting>
        <default>t_paragraph_brk</default>
        <action>token</action>
      </transition>
      <transition>
              <to_state>START</to_state>
              <event>c_dot</event>
              <condition>
                      <after>
                              <event>c_dot</event>
                      </after>
                      <after>
                              <event>c_dot</event>
                      </after>
              </condition>
              <action>+1</action>
              <action>+1</action>
              <action>+1</action>
              <setting>T_SENTENCE_BRK</setting>
              <default>t_sentence_brk</default>
              <action>token</action>
      </transition>
      <transition>
        <to_state>BLANK</to_state>
        <event>c_del2</event>
        <event>c_dot</event>
        <action>+1</action>
        <setting>T_SENTENCE_BRK</setting>
        <default>t_sentence_brk</default>
        <action>token</action>
      </transition>
      <transition>
        <to_state>START</to_state>
        <event>c_del</event>
        <action>+1</action>
        <action>token</action>
      </transition>
      <transition>
        <to_state>START</to_state>
        <event>unknwn</event>
        <action>+1</action>
        <action>trace</action>
      </transition>
      <transition>
        <to_state>END</to_state>
        <event>m_parag</event>
        <action>exit</action>
      </transition>
    </state>
    <state id="ALPHA">
      <internal_status>s_alpha</internal_status>
      <internal_status>s_not_alpha</internal_status>
      <internal_status>s_first_name</internal_status>
      <automaton id="A_MAJ_MIN">
        <!-- must be A_MAJ_MIN-->
        <title>Upper/lower cases automaton</title>
        <!-- no permitted Actions-->
        <!-- 5 permitted Settings : "T_CAPITAL", "T_SMALL", "T_CAPITAL_1ST",
"T_ACRONYM", "T_CAPITAL_SMALL"-->
        <!-- 3 permitted Status : "s_alpha", "s_not_alpha", "s_first_name"-->
        <state id="START_ALPHA">
          <transition>
            <to_state>SINGLE_UPPER</to_state>
            <event>c_M</event>
            <setting>T_CAPITAL</setting>
            <default>t_capital</default>
            <return_status>s_alpha</return_status>
          </transition>
          <transition>
            <to_state>ALL_LOWER</to_state>
            <event>c_m</event>
            <setting>T_SMALL</setting>
            <default>t_small</default>
            <return_status>s_alpha</return_status>
          </transition>
          <transition>
            <to_state>START_ALPHA</to_state>
            <event>all</event>
            <return_status>s_not_alpha</return_status>
          </transition>
        </state>
        <state id="SINGLE_UPPER">
          <transition>
            <to_state>ALL_UPPER</to_state>
            <event>c_M</event>
            <return_status>s_alpha</return_status>
          </transition>
          <transition>
            <to_state>LOWER_1ST_UPPER</to_state>
            <event>c_m</event>
            <setting>T_CAPITAL_1ST</setting>
            <default>t_capital_1st</default>
            <return_status>s_alpha</return_status>
          </transition>
          <transition>
            <to_state>ACRONYM_1</to_state>
            <event>c_dot</event>
            <condition>
              <after>
                <event>c_M</event>
              </after>
              <after>
                <event>c_dot</event>
              </after>
            </condition>
            <setting>T_ACRONYM</setting>
            <default>t_acronym</default>
            <return_status>s_alpha</return_status>
          </transition>
          <transition>
            <to_state>SINGLE_UPPER</to_state>
            <event>c_dot</event>
            <condition>
              <after>
                <event>c_b</event>
                <event>c_M</event>
              </after>
            </condition>
            <setting>T_CAPITAL_1ST</setting>
            <default>t_capital_1st</default>
            <return_status>s_first_name</return_status>
          </transition>
          <transition>
            <to_state>SINGLE_UPPER</to_state>
            <event>all</event>
            <return_status>s_not_alpha</return_status>
          </transition>
        </state>
        <state id="ALL_UPPER">
          <transition>
            <to_state>ALL_UPPER</to_state>
            <event>c_M</event>
            <default>t_capital</default>
            <return_status>s_alpha</return_status>
          </transition>
          <transition>
            <to_state>LOWER_UPPER</to_state>
            <event>c_m</event>
            <setting>T_CAPITAL_SMALL</setting>
            <default>t_capital_small</default>
            <return_status>s_alpha</return_status>
          </transition>
          <transition>
            <to_state>ALL_UPPER</to_state>
            <event>all</event>
            <return_status>s_not_alpha</return_status>
          </transition>
        </state>
        <state id="ALL_LOWER">
          <transition>
            <to_state>ALL_LOWER</to_state>
            <event>c_m</event>
            <return_status>s_alpha</return_status>
          </transition>
          <transition>
            <to_state>LOWER_UPPER</to_state>
            <event>c_M</event>
            <setting>T_CAPITAL_SMALL</setting>
            <default>t_capital_small</default>
            <return_status>s_alpha</return_status>
          </transition>
          <transition>
            <to_state>ALL_LOWER</to_state>
            <event>all</event>
            <return_status>s_not_alpha</return_status>
          </transition>
        </state>
        <state id="LOWER_1ST_UPPER">
          <transition>
            <to_state>LOWER_1ST_UPPER</to_state>
            <event>c_m</event>
            <default>t_capital_1st</default>
            <return_status>s_alpha</return_status>
          </transition>
          <transition>
            <to_state>LOWER_UPPER</to_state>
            <event>c_M</event>
            <setting>T_CAPITAL_SMALL</setting>
            <default>t_capital_small</default>
            <return_status>s_alpha</return_status>
          </transition>
          <transition>
            <to_state>LOWER_1ST_UPPER</to_state>
            <event>all</event>
            <return_status>s_not_alpha</return_status>
          </transition>
        </state>
        <state id="LOWER_UPPER">
          <transition>
            <to_state>LOWER_UPPER</to_state>
            <event>c_Mm</event>
            <return_status>s_alpha</return_status>
          </transition>
          <transition>
            <to_state>LOWER_UPPER</to_state>
            <event>all</event>
            <return_status>s_not_alpha</return_status>
          </transition>
        </state>
        <state id="ACRONYM_1">
          <transition>
            <to_state>ACRONYM_2</to_state>
            <event>all</event>
            <return_status>s_alpha</return_status>
          </transition>
        </state>
        <state id="ACRONYM_2">
          <transition>
            <to_state>ACRONYM_1</to_state>
            <event>c_dot</event>
            <condition>
              <after>
                <event>c_M</event>
              </after>
              <after>
                <event>c_del</event>
                <event>m_line</event>
              </after>
            </condition>
            <return_status>s_alpha</return_status>
          </transition>
          <transition>
            <to_state>ACRONYM_2</to_state>
            <event>all</event>
            <return_status>s_not_alpha</return_status>
          </transition>
        </state>
      </automaton>
      <automaton id="A_ROMAN">
        <!-- must be A_ROMAN-->
        <title>Roman numbers automaton</title>
        <!-- no permitted Actions-->
        <!-- 3 permitted Settings : "T_CARDINAL_ROMAN", "T_ORDINAL_ROMAN", "T_NOT_ROMAN"-->
        <state id="START_ROMAN">
          <transition>
            <to_state>CARDINAL_ROMAN</to_state>
            <event>c_V</event>
            <setting>T_CARDINAL_ROMAN</setting>
            <default>t_cardinal_roman</default>
          </transition>
          <transition>
            <to_state>NOT_ROMAN</to_state>
            <event>all</event>
            <setting>T_NOT_ROMAN</setting>
          </transition>
        </state>
        <state id="CARDINAL_ROMAN">
          <transition>
            <to_state>CARDINAL_ROMAN</to_state>
            <event>c_V</event>
            <default>t_cardinal_roman</default>
          </transition>
          <transition>
            <to_state>ORDINAL_ROMAN</to_state>
            <event>c_o</event>
            <setting>T_ORDINAL_ROMAN</setting>
            <default>t_ordinal_roman</default>
          </transition>
          <transition>
            <to_state>NOT_ROMAN</to_state>
            <event>c_Mm</event>
            <setting>T_NOT_ROMAN</setting>
          </transition>
          <transition>
            <to_state>NOT_ROMAN</to_state>
            <event>all</event>
            <setting>T_NOT_ROMAN</setting>
          </transition>
        </state>
        <state id="ORDINAL_ROMAN">
          <transition>
            <to_state>ORDINAL_ROMAN</to_state>
            <event>c_o</event>
            <default>t_ordinal_roman</default>
          </transition>
          <transition>
            <to_state>NOT_ROMAN</to_state>
            <event>c_Mm</event>
            <setting>T_NOT_ROMAN</setting>
          </transition>
          <transition>
            <to_state>NOT_ROMAN</to_state>
            <event>all</event>
            <setting>T_NOT_ROMAN</setting>
          </transition>
        </state>
        <state id="NOT_ROMAN">
          <transition>
            <to_state>NOT_ROMAN</to_state>
            <event>all</event>
          </transition>
        </state>
      </automaton>
      <transition>
        <to_state>START</to_state>
        <event>c_Mm</event>
        <condition>
          <after>
            <event>c_hyphen</event>
          </after>
          <after>
            <event>c_a_t</event>
          </after>
          <after>
            <event>c_hyphen</event>
          </after>
        </condition>
        <action>+1</action>
        <action>token</action>
      </transition>
      <transition>
        <to_state>ALPHA</to_state>
        <event>all</event>
        <condition>
          <status>s_alpha</status>
        </condition>
        <action>+1</action>
      </transition>
      <transition>
        <to_state>ALPHA</to_state>
        <event>c_lowline</event>
        <action>+1</action>
      </transition>
      <transition>
        <to_state>ALPHA</to_state>
        <event>c_degree</event>
        <condition>
          <before>
            <event>c_N</event>
            <event>c_n</event>
          </before>
        </condition>
        <action>+1</action>
        <action>token</action>
      </transition>
      <transition>
        <to_state>ALPHA</to_state>
        <event>c_hyphen</event>
        <condition>
          <after>
            <event>c_Mm</event>
            <event>m_line</event>
          </after>
        </condition>
        <action>+1</action>
        <setting>T_HYPHEN_WORD</setting>
      </transition>
      <transition>
        <to_state>ALPHA</to_state>
        <event>c_quote</event>
        <condition>
          <after>
            <event>c_S</event>
            <event>c_s</event>
          </after>
          <after>
            <event>c_del</event>
            <event>m_line</event>
          </after>
        </condition>
        <action>+1</action>
      </transition>
      <transition>
        <to_state>ALPHA</to_state>
        <event>c_quote</event>
        <condition>
          <before>
            <event>c_O</event>
            <event>c_l_o</event>
            <event>c_N</event>
          </before>
          <after>
            <event>c_M</event>
          </after>
        </condition>
        <action>+1</action>
      </transition>
      <transition>
        <to_state>ALPHA</to_state>
        <event>c_quote</event>
        <condition>
          <after>
            <event>c_b</event>
          </after>
        </condition>
        <action>+1</action>
      </transition>
      <transition>
        <to_state>ALPHA</to_state>
        <event>c_quote</event>
        <condition>
          <after>
            <event>c_del1</event>
          </after>
        </condition>
        <action>+1</action>
      </transition>
      <transition>
        <to_state>ALPHA</to_state>
        <event>unknwn</event>
        <action>+1</action>
        <action>trace</action>
      </transition>
      <transition>
        <to_state>ALPHANUMERIC</to_state>
        <event>c_5</event>
        <action>+1</action>
        <setting>T_ALPHANUMERIC</setting>
        <default>t_alphanumeric</default>
      </transition>
      <transition>
        <to_state>START</to_state>
        <event>c_quote</event>
        <action>+1</action>
        <action>token</action>
      </transition>
      <transition>
        <to_state>START</to_state>
        <event>c_dot</event>
        <condition>
          <after>
            <event>c_b</event>
          </after>
          <after>
            <event>c_m</event>
          </after>
        </condition>
        <action>+1</action>
        <action>token</action>
      </transition>
      <transition>
        <to_state>BLANK</to_state>
        <event>all</event>
        <condition>
          <status>s_first_name</status>
        </condition>
        <action>+1</action>
        <action>token</action>
      </transition>
      <transition>
        <to_state>START</to_state>
        <event>all</event>
        <condition>
          <status>s_not_alpha</status>
        </condition>
        <action>token</action>
      </transition>
      <transition>
        <to_state>END</to_state>
        <event>m_parag</event>
        <action>token</action>
        <action>exit</action>
      </transition>
    </state>
    <state id="NUMERIC">
      <internal_status>s_numeric</internal_status>
      <internal_status>s_not_numeric</internal_status>
      <automaton id="A_NUMBER">
        <!-- must be A_NUMBER-->
        <title>Numbers automaton</title>
        <!-- no permitted Actions-->
        <!-- 5 permitted Settings : "T_INTEGER", "T_COMMA_NUMBER", "T_DOT_NUMBER", 
"T_FRACTION", "T_ORDINAL_INTEGER"-->
        <state id="START_NUMERIC">
          <transition>
            <to_state>INTEGER</to_state>
            <event>c_5</event>
            <setting>T_INTEGER</setting>
            <default>t_integer</default>
            <return_status>s_numeric</return_status>
          </transition>
          <transition>
            <to_state>START_NUMERIC</to_state>
            <event>all</event>
            <return_status>s_not_numeric</return_status>
          </transition>
        </state>
        <state id="INTEGER">
          <transition>
            <to_state>INTEGER</to_state>
            <event>c_5</event>
            <return_status>s_numeric</return_status>
          </transition>
          <transition>
            <to_state>COMMA_NUMBER</to_state>
            <event>c_comma</event>
            <condition>
              <after>
                <event>c_5</event>
              </after>
            </condition>
            <setting>T_COMMA_NUMBER</setting>
            <default>t_comma_number</default>
            <return_status>s_numeric</return_status>
          </transition>
          <transition>
            <to_state>DOT_NUMBER</to_state>
            <event>c_dot</event>
            <condition>
              <after>
                <event>c_5</event>
              </after>
            </condition>
            <setting>T_DOT_NUMBER</setting>
            <default>t_dot_number</default>
            <return_status>s_numeric</return_status>
          </transition>
          <transition>
            <to_state>FRACTION</to_state>
            <event>c_slash</event>
            <condition>
              <after>
                <event>c_5</event>
              </after>
            </condition>
            <setting>T_FRACTION</setting>
            <default>t_fraction</default>
            <return_status>s_numeric</return_status>
          </transition>
          <transition>
            <to_state>ORDINAL_INTEGER</to_state>
            <event>c_o</event>
            <setting>T_ORDINAL_INTEGER</setting>
            <default>t_ordinal_integer</default>
            <return_status>s_numeric</return_status>
          </transition>
          <transition>
            <to_state>INTEGER</to_state>
            <event>all</event>
            <return_status>s_not_numeric</return_status>
          </transition>
        </state>
        <state id="COMMA_NUMBER">
          <transition>
            <to_state>COMMA_NUMBER</to_state>
            <event>c_5</event>
            <return_status>s_numeric</return_status>
          </transition>
          <transition>
            <to_state>COMMA_NUMBER</to_state>
            <event>all</event>
            <return_status>s_not_numeric</return_status>
          </transition>
        </state>
        <state id="DOT_NUMBER">
          <transition>
            <to_state>DOT_NUMBER</to_state>
            <event>c_5</event>
            <return_status>s_numeric</return_status>
          </transition>
          <transition>
            <to_state>DOT_NUMBER</to_state>
            <event>c_dot</event>
            <condition>
              <after>
                <event>c_5</event>
              </after>
            </condition>
            <return_status>s_numeric</return_status>
          </transition>
          <transition>
            <to_state>DOT_NUMBER</to_state>
            <event>all</event>
            <return_status>s_not_numeric</return_status>
          </transition>
        </state>
        <state id="FRACTION">
          <transition>
            <to_state>FRACTION</to_state>
            <event>c_5</event>
            <return_status>s_numeric</return_status>
          </transition>
          <transition>
            <to_state>FRACTION</to_state>
            <event>all</event>
            <return_status>s_not_numeric</return_status>
          </transition>
        </state>
        <state id="ORDINAL_INTEGER">
          <transition>
            <to_state>ORDINAL_INTEGER</to_state>
            <event>c_o</event>
            <return_status>s_numeric</return_status>
          </transition>
          <transition>
            <to_state>ORDINAL_INTEGER</to_state>
            <event>all</event>
            <return_status>s_not_numeric</return_status>
          </transition>
        </state>
      </automaton>
      <transition>
        <to_state>NUMERIC</to_state>
        <event>all</event>
        <condition>
          <status>s_numeric</status>
        </condition>
        <action>+1</action>
      </transition>
      <transition>
        <to_state>NUMERIC</to_state>
        <event>c_lowline</event>
        <condition>
          <status>s_numeric</status>
        </condition>
        <action>+1</action>
      </transition>
      <transition>
        <to_state>NUMERIC</to_state>
        <event>unknwn</event>
        <action>+1</action>
        <action>trace</action>
      </transition>
      <transition>
        <to_state>ALPHANUMERIC</to_state>
        <event>c_Mm</event>
        <condition>
          <status>s_not_numeric</status>
        </condition>
        <action>+1</action>
        <setting>T_ALPHANUMERIC</setting>
        <default>t_alphanumeric</default>
      </transition>
      <transition>
        <to_state>START</to_state>
        <event>all</event>
        <condition>
          <status>s_not_numeric</status>
        </condition>
        <action>token</action>
      </transition>
      <transition>
        <to_state>BLANK</to_state>
          <event>all</event>
          <action>+1</action>
      </transition>
      <transition>
        <to_state>END</to_state>
        <event>m_parag</event>
        <action>token</action>
        <action>exit</action>
      </transition>
    </state>
    <state id="PATTERN">
      <transition>
        <to_state>START</to_state>
        <event>m_end_pattern</event>
        <action>+1</action>
        <action>token</action>
      </transition>
      <transition>
        <to_state>PATTERN</to_state>
        <event>all</event>
        <action>+1</action>
      </transition>
      <transition>
        <to_state>PATTERN</to_state>
        <event>unknwn</event>
        <action>+1</action>
        <action>trace</action>
      </transition>
      <transition>
